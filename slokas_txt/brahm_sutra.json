{
    "१-१-१": "अथातो ब्रह्मजिज्ञासा ॥",
    "१-१-२": "जन्माद्यस्य यतः ॥",
    "१-१-३": "शास्त्रयोनित्वात् ॥",
    "१-१-४": "तत्तु समन्वयात् ॥",
    "१-१-५": "ईक्षतेर्नाशब्दम् ॥",
    "१-२-६": "स्मृतेश्च ॥",
    "१-१-१३": "आनन्दमयोऽभ्यासात् ॥",
    "१-१-१४": "विकारशब्दान् नेति चेन् न प्राचुर्यात्",
    "१-१-१८": "भेदव्यपदेशाच्च ॥",
    "१-१-२०": "अस्मिन्नस्य च तद्योगं शास्ति ॥",
    "१-१-२१": "अन्तस्तद्धर्मोपदेशात् ॥",
    "१-१-२२": "भेदव्यपदेशाच्चान्यः ॥",
    "१-२-१": "सर्वत्र प्रसिद्धोपदेशात् ॥",
    "१-२-२": "विवक्षितगुणोपपत्तेश्च ॥",
    "१-२-५": "शब्दविशेषात् ॥",
    "१-२-८": "सम्भोगप्राप्तिरिति चैन्न वैशेष्यात् ॥",
    "१-२-२१": "उभयेऽपि हि भेदेनैनमधीयते ॥",
    "१-३-२": "मुक्तोपसृप्यव्यपदेशात् च",
    "१-३-४": "भेदव्यपदेशात् ॥",
    "१-३-६": "स्थित्यदनाभ्यां च ॥",
    "१-३-१३": "दहर उत्तरेभ्यः ॥",
    "१-३-१४": "गतिशब्दाभ्यां तथा हि दृष्टं लिङ्गं च ॥",
    "१-३-१५": "धृतेश्च महिम्नोऽस्यास्मिन्नुपलब्धेः ॥",
    "१-३-१६": "प्रसिद्धेश्च ॥",
    "१-३-१७": "इतरपरामर्शात्स इति चेन्नासम्भवात् ॥",
    "१-३-१८": "उत्तराच्चेदाविर्भूतस्वरूपस्तु ॥",
    "१-३-४३": "सुषुप्त्युत्क्रान्त्योर्भेदेन ॥",
    "१-३-४४": "पत्यादिशब्देभ्यः ॥",
    "१-४-२२": "अवस्थितेरिति काशकृत्स्नः",
    "१-४-२६": "आत्मकृतेः परिणामात् ॥",
    "१-४-२७": "योनिश्च हि गीयते ॥",
    "२-१-११": "तर्काप्रतिष्ठानात् ॥",
    "२-१-१९": "यथा च प्राणादिः ॥",
    "२-१-२२": "अधिकं तु भेदनिर्देशात् ॥",
    "२-१-२३": "अश्मादिवच्च तदनुपपत्तिः ॥",
    "२-१-२७": "श्रुतेस्तु शब्दमूलत्वात् ॥",
    "२-१-२८": "आत्मनि चेव विचित्राश्च हि ॥",
    "२-१-३०": "सर्वोपेता च तद्दर्शनात् ॥",
    "२-१-३३": "लोकवत्तु लीलाकैवल्यम् ॥",
    "२-१-३४": "वैषम्यनैर्घृण्ये न सापेक्षत्वात्तथाहि दर्शयति ॥",
    "२-१-३५": "न कर्माविभागादिति चेन्नानादित्वात् ॥",
    "२-२-२": "प्रवृत्तेश्च ॥",
    "२-२-३१": "क्षणिकत्वात् ॥",
    "२-२-३२": "एवं चात्माकार्त्स्यम् ॥",
    "२-२-३३": "न च पर्यायादप्यविरोधो विकारादिभ्यः ॥",
    "२-२-३४": "अन्त्यावस्थितेश्चोभयनित्यत्वादविशेषः ",
    "२-२-३८": "सम्बन्धानुपपतेश्च ॥",
    "२-३-१८": "नात्माऽश्रुतेर्नित्यत्वाच्च ताभ्यः ॥",
    "२-३-१९": "ज्ञोऽत एव ॥",
    "२-३-२०": "उत्क्रान्ति गत्यागतीनाम् ॥",
    "२-३-२१": "स्वात्मना चोत्तरयोः ॥",
    "२-३-२२": "नाणुरतच्छ्रुतेरिति चेन्नेतराधिकारात् ॥",
    "२-३-२३": "स्वशब्दोन्मानाभ्याञ्च ॥",
    "२-३-२४": "अविरोधश्चन्दनवत् ॥",
    "२-३-२५": "अवस्थितिवैशेष्यादिति चेन्नाभ्युपगमाद् हृदि हि ॥",
    "२-३-२६": "गुणाद्वालोकवत् ॥",
    "२-३-२७": "व्यतिरेको गन्धवत् ॥",
    "२-३-२८": "पृथगुपदेशात् ॥",
    "२-३-२९": "तद्गुणसारत्वात्तु तद्व्यपदेशः प्राज्ञवत् ॥",
    "२-३-३०": "पुंस्त्वादिवत्त्वस्य सतोऽभिव्यक्तियोगात् ॥",
    "२-३-३१": "पुंस्त्वादिवत्त्वस्य सतोऽभिव्यक्तियोगात् ॥",
    "२-३-३३": "कर्ता शास्त्रार्थवत्त्वात् ॥",
    "२-३-३४": "अन्त्यावस्थितेश्चोभयनित्यत्वादविशेषः ॥",
    "२-३-३५": "विहारोपदेशात् ॥",
    "२-३-३६": "उपलब्धिवदनियमः ॥",
    "२-३-३७": "शक्तिविपर्ययात् ॥",
    "२-३-३८": "समाध्यभावाच्च ॥",
    "२-३-३९": "यथा च तक्षोभयथा ॥",
    "२-३-४०": "परात्तु तच्छ्रुतेः ॥",
    "२-३-४१": "कृतप्रयत्नापेक्षस्तु विहितप्रतिषिद्धावैयर्थ्यादिभ्यः ॥",
    "२-३-४२": "अंशो नानाव्यपदेशात्॥",
    "२-३-४३": "मन्त्रवर्णाच्च ॥",
    "२-३-४४": "अपि च स्मर्यते ॥",
    "२-३-४५": "प्रकाशादिवत् नैवं परः ॥",
    "२-३-४६": "स्मरन्ति च ॥",
    "३-१-२०": "दर्शनाच्च ॥",
    "३-२-४": "सूचकश्च हि श्रुतेराचक्षते च तद्विदः ॥",
    "३-२-१३": "अपि चैवमेके ॥",
    "३-२-१५": "प्रकाशवच्चावैयर्थ्यात् ॥",
    "३-२-२२": "तदव्यक्तमाह हि ॥",
    "३-२-२३": "अपि च संराधने प्रत्यक्षानुमानाभ्याम्",
    "३-२-२४": "प्रकाशादिवच्चावैशेष्यं प्रकाशश्च कर्मण्यभ्यासात् ॥",
    "३-२-२५": "अतोऽनन्तेन तथा हि लिङ्गम् ॥",
    "३-२-२६": "उभयव्यपदेशात्त्वहिकुण्डलवत् ॥",
    "३-२-२७": "प्रकाशाश्रयवद्वा तेजस्त्वात् ॥",
    "३-४-८": "अधिकोपदेशात्तु॥",
    "४-१-१": "आवृत्तिरसकृदुपदेशात् ॥",
    "४-१-७": "आसीनः सम्भवात्",
    "४-१-८": "ध्यानाच्च",
    "४-१-९": "अचलत्वं चापेक्ष्य",
    "४-१-१०": "स्मरन्ति च",
    "४-१-११": "यत्रैकाग्रता तत्राविशेषात्",
    "४-१-१२": "आप्रायणात्तत्रापि हि दृष्टम्",
    "४-३-१०": "स्मृतेश्च ॥",
    "४-४-११": "द्वादशाहवदुभयविधं बादरायणोऽतः ॥",
    "४-४-१७": "जगद्व्यापारवर्जम्॥",
    "४-४-२१": "भोगमात्रसाम्यलिङ्गाच्च ॥"
}
